{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y",
    "react",
    "prettier"
  ],
  "rules": {
    "semi": [2, "never"],
    "indent": ["error", 2],
    "react/destructuring-assignment": "off",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-underscore-dangle": "off",
    "react/forbid-prop-types": "off"
  }
}